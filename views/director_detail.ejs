<%- include header %>

    <div class="content">
        <h1 class="movie_heading"><%= director.first_name %> <%= director.last_name %></h1>

        <div class="movie_detail">
            <p><span  class="sub-heading">Date of Birth: </span> <%= director.dob %></p>
            <p><span  class="sub-heading">Age: </span> <%= director.age %></p>
            <p><span  class="sub-heading">Movies:</p>
            
            <ul>
                <% director_movies.forEach(function(movie)  { %>
                    <li class="director_movies"><a href=<%= movie.url %> ><%= movie.name %></a></li>
                <% }) %>

            </ul>
            
        </div>
    </div>
    
    <body>
<html>


